package com.a3.learnings;


class Test<T>

{
	T obj;
	public Test(T obj) {
		this.obj = obj;
	}
	
	public T getObject() {
		return this.obj;
	}
}
public class Main {
	
	public static void main (String args[]) {
		
		
		//Instance of integer type
		Test<Integer> 	iobj = new Test<Integer>(1);
		System.out.println("Printing the object---"+iobj.getObject());
	}
	
	

}
