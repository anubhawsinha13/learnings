package com.a3.learnings.model;



class Movie{
	
	private String name;
	
	public Movie(String name) {
		
		this.name = name;
	}
	
	public String plot() {
		
		return "no plot here";
	}
}


class Jaws extends Movie{
		
	public Jaws() {
		
		super("Jaws");
	}
	
	public String Plot() {
		
		return "A shark eats lots of people";
	}
	
}


